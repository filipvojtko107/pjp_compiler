program PJP9(IN, OUT);
var
    cnt1, cnt2, i, j : integer;

begin
    cnt1 := 0;
    cnt2 := 0;
    i := 5;
    j := 7;

	while (i >= 0) do
	begin
		if (i > 3) then
			cnt1 := cnt1 + 4
		else
			cnt1 := cnt1 + 1;
		i := i - 1
	end;

	while (j >= 0) do
	begin
		if (j > 5) then
			cnt2 := cnt2 + 3
		else
			cnt2 := cnt2 + 1;
		j := j - 1
	end
end.
